{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "open-emacs",
  "title": "Open Emacs",
  "description": "Launches Emacs from local path",
  "icon": "emacs_512.png",
  "author": "Kerim",
  "license": "MIT",
  "commands": [
    {
      "name": "emacs",
      "title": "Launch Emacs",
      "description": "Opens the local Emacs executable",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}